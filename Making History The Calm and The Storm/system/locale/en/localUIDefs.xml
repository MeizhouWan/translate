	<objectDef type="DateFormat" class="CUnstoredObject">
		<!-- Inputs -->
		<propertyDef name="dateInput" type="Date"/>

		<!-- Outputs -->
		<propertyDef name="strDate" type="String"/>
	</objectDef>
	
	<objectDef type="DateFormatThreeLetterMonthWithComma" superType="DateFormat">
		<property name="strDate" computeFrom="getMonthString(this.dateInput) + ' ' + cast(String, getDay(this.dateInput)) + ', ' + cast(String, getYear(this.dateInput))"/>
	</objectDef>

	<objectDef type="DateFormatThreeLetterMonthNoComma" superType="DateFormat">
		<property name="strDate" computeFrom="getMonthString(this.dateInput) + ' ' + cast(String, getDay(this.dateInput)) + ' ' + cast(String, getYear(this.dateInput))"/>
